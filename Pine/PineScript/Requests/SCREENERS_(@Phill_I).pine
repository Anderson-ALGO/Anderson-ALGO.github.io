conditions_phill(o_pair, h_pair, l_pair, c_pair)=>
    float EMA1    =   ema(c_pair,len1)
    float EMA2    =   ema(c_pair,len2)
    float EMA3    =   ema(c_pair,len3)
    float _close  =   c_pair
    float _open   =   o_pair   
    float _high   =   h_pair
    float _low    =   l_pair
    float condition          =   iff((_close>_open and _close>_close[1] and _close>_high[1] and _close[1]<_open[1] and EMA1>EMA2),  c_pair, -c_pair )
    float alertcondition     =   iff((_close>_open and _close>_close[1] and _close>_high[1] and _close[1]<_open[1] and EMA1>EMA2),  1, 0 )
    //float Sell    =   iff((_close<_open and _close<_close[1] and _close<_low[1]  and _close[1]>_open[1] and EMA1<EMA2), -c_pair, na )
    [condition, alertcondition]

