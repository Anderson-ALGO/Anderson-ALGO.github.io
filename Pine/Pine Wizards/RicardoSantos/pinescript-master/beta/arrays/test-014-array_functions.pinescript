f_array_stdev(_id)=>
    int _size = array.size(id=_id)
    float _mean = array.sum(id=_id) / _size
    float _dev = 0.0
    for _i = 0 to _size - 1
        _dev := _dev + abs(array.get(id=_id, index=_i) - _mean)
    _dev := _dev / _size

