f_array_variance(_id)=>
    int _size = array.size(id=_id)
    float _mean = array.sum(id=_id) / _size
    float _var = 0.0
    for _i = 0 to _size - 1
        _var := _var + pow(array.get(id=_id, index=_i) - _mean, 2)
    _var / _size
    
